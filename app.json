{
  "expo": {
    "name": "Ohana",
    "slug": "OhanaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ohanalogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.OhanaApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ohanalogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.OhanaApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ohanalogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ohanalogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9fbc861b-20ec-4484-a868-df0131eddf30"
      }
    }
  }
}
